useDynLib(XBART, .registration=TRUE)
importFrom(Rcpp, evalCpp)
importFrom(stats, predict, pnorm, quantile, runif, sd, var)
importFrom(tools, psnice)
S3method(predict, XBART)
S3method(predict, XBARTmultinomial)
S3method(predict, XBCFcontinuous)
S3method(predict, XBCFdiscrete)
S3method(predict,XBARTheteroskedastic)
exportPattern("^[[:alpha:]]+")
